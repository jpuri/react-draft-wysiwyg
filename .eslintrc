{
  "parser": "babel-eslint",
  "plugins": [ "mocha" ],
  "extends": "airbnb",
  "globals": {
    "describe": true,
    "it": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "react/no-array-index-key": 0,
    "react/require-default-props": 0,
    "import/no-unresolved": 2,
    "linebreak-style": [
      "error",
      "windows"
    ]
  }
}

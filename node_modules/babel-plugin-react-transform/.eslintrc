{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": "eslint:recommended",
  "rules": {
    "strict": 0,
    "curly": [1, "multi-line"],
    "camelcase": 0,
    "comma-dangle": 1,
    "no-use-before-define": [1, "nofunc"],
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "semi": 1
  },
  env: {
    "node": true
  }
}

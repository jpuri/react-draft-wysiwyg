{
    "extends": "@ljharb",
    "rules": {
        "indent": [2, 4],
        "strict": 0,
        "curly": 0,
        "max-params": 0,
        "max-statements": 0,
        "no-magic-numbers": 0,
        "global-require": 0,
        "no-console": 0,
        "no-shadow": 0,
        "no-unused-vars": [2, { "vars": "all", "args": "none" }],
        "sort-keys": 0,
        "max-statements-per-line": [2, { "max": 2 }],
        "func-name-matching": 0,
        "func-style": 0,
        "object-curly-newline": 0,
        "no-use-before-define": 1,
        "complexity": 0,
        "consistent-return": 0
    }
}

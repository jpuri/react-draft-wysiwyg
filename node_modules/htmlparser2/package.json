  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "istanbul": "^0.4.3",
    "mocha": "^2.2.5",
    "eslint": "^2.12.0",
    "mocha-lcov-reporter": "^1.2.0"
  },
  "browser": {
